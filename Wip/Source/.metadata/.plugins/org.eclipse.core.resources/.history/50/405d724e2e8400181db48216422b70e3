void main(void);

void main(void)
{
	//init module here

    while (1)
    {
		switch (g_state)
		{
			case IDLE_STATE:
			 if(s_is_unlock)
			 {
				 g_state = SETTING_STATE;
			 }
			break;
			case SETTING_STATE:
			// @ quan: handle here
			break;
			case CALIB_TDS_STATE:

			break;
			default :
			break;
		}
    }
}
